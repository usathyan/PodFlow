# PodFlow Release Signing Configuration
# Copy this file to keystore.properties and fill in your actual values
# DO NOT commit keystore.properties to version control!

# Path to your keystore file (absolute or relative to project root)
releaseStoreFile=/path/to/your/podflow-release.keystore

# Keystore password
releaseStorePassword=your_keystore_password

# Key alias in the keystore
releaseKeyAlias=podflow

# Key password
releaseKeyPassword=your_key_password
